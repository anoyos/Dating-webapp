<style>
    input.header-search::-webkit-input-placeholder {
        font-size: 20px
    }




    /*story listing---------------------------start*/
    .color-btn-sty {
        color: white;
        background: #59D6D6;
        border-radius: 10px;
        font-size: 15px;
    }

    .gray-btn-sty {
        color: white;
        background: #989898;
        font-weight: 700;
        width: 350px;
        font-size: 20px;
    }

    .card-detail h3 {
        color: white;
    }

    .story-card {
        background-size: contain;
        padding: 30px 25px;
        color: white;
    }

    .top-space {
        margin-bottom: 328px;
    }

        .top-space img {
            width: 10px;
            position: relative;
            top: -2px;
        }

    .card-detail .per-dts ul li {
        font-size: 18px;
        margin-bottom: 5px;
        color: white;
    }

        .card-detail .per-dts ul li img {
            width: 23px;
            margin-right: 6px;
        }

        .card-detail .per-dts ul li span {
            float: right;
        }

    .link-btn-icon img {
        width: 80%;
        background: rgb(89 214 214 / 74%);
        border-radius: 10px;
        padding: 15px;
    }

        .link-btn-icon img:hover {
            background: none;
            cursor: pointer
        }

    .link-btn-icon .list-inline .list-inline-item {
        display: table-cell;
    }

    .link-btn-icon {
        margin-top: 60px;
    }



    /*Extra css*/
    .extra-class {
        height: 740px;
        width: 470px;
        margin: auto;
        margin-top: 20px;
    }

    .story-card {
        background-size: cover !important;
        border-radius: 30px !important;
    }
    /*end ---------------------------------*/



    .modal-dialog {
        max-width: 700px;
        margin: 1.75rem auto;
    }

    .story-card.extra-class > img {
        position: absolute;
        bottom: 0px;
        left: 0px;
        height: 470px;
    }

    .story-card.extra-class {
        position: relative;
        z-index: 1;
        box-shadow: rgb(0 0 0 / 16%) 0px 3px 6px;
    }

    .card-detail {
        position: relative;
        z-index: 2;
    }

    .link-btn-icon img {
        width: 100%;
        background: none;
        border-radius: 0px;
        padding: 0px;
    }

