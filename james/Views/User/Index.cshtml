        justify-content: center;
    }

    link-btn-icon .list-inline.list-inline-center .list-inline-item {
        display: table-cell;
        margin: 0px 10px;
    }

    .tag-yellow img {
        width: 27px;
        margin-right: 5px;
    }

    .tag-yellow {
        background-color: #ffbb04;
        color: #fff;
        position: absolute;
        right: 0px;
        padding: 7px 20px 2px 10px;
        border-radius: 10px 0px 0px 10px;
    }

    .tag-blue img {
        width: 20px;
        margin-right: 5px;
        height: 20px;
    }

    .tag-blue {
        background-color: #0492FF;
        color: #fff;
        position: absolute;
        right: 0px;
        padding: 9px 20px 5px 14px;
        border-radius: 10px 0px 0px 10px;
        top: 80px;
    }

