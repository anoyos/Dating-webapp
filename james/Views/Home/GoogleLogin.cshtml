
@{
    Layout = null;
    ViewBag.Title = "404";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Google Login</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  
    <link rel="stylesheet" href="~/adminlte/css/skins/_all-skins.min.css">
   
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Login
                </h1>
            </section>
            <!-- Main content -->
            <section class="content">
                <div class="error-page">
                    <h2 class="headline text-yellow"> Google Login</h2>
                    @if (ViewBag.message != null)
                    {
                        <div class="error-content">
                            <h3><i class="fa fa-warning text-yellow"></i> @ViewBag.message</h3>
                            <p>
                                <a href="/home/index">return to home</a>.
                            </p>
                        </div>
                    }
                    else
                    {

                        <script>
                            if (window.location.href.indexOf('#') > -1) {
                                var url = window.location.href.replace('#', '?');
                                window.location.replace(url);
                            }

                        </script>
                        <div class="success-content">
                            <h3><i class="fa fa-warning text-yellow"></i> Proccessing Login</h3>

                        </div>
                    }

                    <!-- /.error-content -->
                </div>
                <!-- /.error-page -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="/Admin/assets/js/vendors/jquery-3.4.0.min.js"></script>

</body>
</html>

